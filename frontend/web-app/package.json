{
  "name": "roiplatform-web-app",
  "version": "1.0.0",
  "description": "ROIPLATFORM Vue 3 웹 클라이언트 - Ocean Blue & Neumorphism 디자인 시스템",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --port 8080",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --fix",
    "lint:style": "stylelint **/*.{vue,css,scss} --fix",
    "format": "prettier --write .",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist node_modules/.vite",
    "analyze": "vite-bundle-analyzer",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "vue": "^3.4.29",
    "vue-router": "^4.4.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "@vueuse/core": "^10.11.0",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.7.2",
    "@tanstack/vue-query": "^5.45.1",
    "vue-i18n": "^9.13.1",
    "dayjs": "^1.11.11",
    "lodash-es": "^4.17.21",
    "zod": "^3.23.8",
    "@vee-validate/zod": "^4.13.1",
    "vee-validate": "^4.13.1",
    "floating-vue": "^5.2.2",
    "vue-sonner": "^1.1.3",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "nprogress": "^0.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "vite": "^5.3.1",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-windicss": "^1.9.3",
    "vite-bundle-analyzer": "^0.10.1",
    "vue-tsc": "^2.0.21",
    "typescript": "^5.5.2",
    "@types/node": "^20.14.9",
    "@types/lodash-es": "^4.17.12",
    "@types/nprogress": "^0.2.3",
    "vitest": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^24.1.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^9.6.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.3.2",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "rimraf": "^5.0.7",
    "windicss": "^3.5.6",
    "@storybook/vue3": "^8.1.11",
    "@storybook/vue3-vite": "^8.1.11",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-a11y": "^8.1.11",
    "storybook": "^8.1.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "keywords": [
    "vue3",
    "typescript",
    "vite",
    "pinia",
    "roiplatform",
    "saas",
    "multi-tenant",
    "ocean-blue",
    "neumorphism",
    "accessibility"
  ],
  "author": "ROIPLATFORM Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roiplatform/roiplatform-v2.git"
  },
  "homepage": "https://roiplatform.com"
}